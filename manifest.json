{
  "manifest_version": 3,
  "name": "Auto Mute Other Google Meet Tabs",
  "version": "1.0",
  "description": "Mutes all other Google Meet tabs when you switch to one.",
  "permissions": ["scripting", "tabs", "activeTab", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://meet.google.com/*"],
  "icons": {
    "128": "icon.png"
  },
  "action":{
    "default_popup": "popup.html"
  }
}
